<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/focusskulistview_chartcontentarea"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/focusskulistview_chartcontentarea1"
        android:layout_marginLeft="1sp"
        android:layout_marginRight="1sp"
        android:weightSum="1.0" >

        <RelativeLayout
            android:id="@+id/focusskulistview_1C"
            android:layout_width="0dip"
            android:layout_height="55sp"
            android:layout_marginBottom="1sp"
            android:layout_marginRight="1sp"
            android:layout_weight=".25"
            android:background="#f1f1f1"
            android:padding="1sp" >

            <TextView
                android:id="@+id/focusskulistview_1c_titletext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:paddingLeft="5sp"
                android:text="Product"
                android:textColor="#272727"
                android:textSize="15sp"
                android:textStyle="normal" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/focusskulistview_2C"
            android:layout_width="0dip"
            android:layout_height="55sp"
            android:layout_marginBottom="1sp"
            android:layout_marginRight="1sp"
            android:layout_weight=".12"
            android:background="#e1e1e1"
            android:padding="1sp" >

            <EditText
                android:id="@+id/focusskulistview_edit1"
                android:layout_width="fill_parent"
                android:layout_height="55sp"
                android:layout_centerInParent="true"
                android:minLines="1" >
            </EditText>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/focusskulistview_3C"
            android:layout_width="0dip"
            android:layout_height="55sp"
            android:layout_marginBottom="1sp"
            android:layout_marginRight="1sp"
            android:layout_weight=".12"
            android:background="#f1f1f1"
            android:padding="1sp" >

            <EditText
                android:id="@+id/focusskulistview_edit2"
                android:layout_width="fill_parent"
                android:layout_height="55sp"
                android:layout_centerInParent="true"
                android:ems="2" >
            </EditText>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/focusskulistview_4C"
            android:layout_width="0dip"
            android:layout_height="55sp"
            android:layout_marginBottom="1sp"
            android:layout_marginRight="1sp"
            android:layout_weight=".12"
            android:background="#e1e1e1"
            android:padding="1sp" >

            <EditText
                android:id="@+id/focusskulistview_edit3"
                android:layout_width="fill_parent"
                android:layout_height="55sp"
                android:layout_centerInParent="true"
                android:ems="2" >
            </EditText>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/focusskulistview_4C"
            android:layout_width="0dip"
            android:layout_height="55sp"
            android:layout_marginBottom="1sp"
            android:layout_weight=".13"
            android:background="#f1f1f1" >

            <Button
                android:id="@+id/dom1"
                android:layout_width="fill_parent"
                android:layout_height="55sp"
                android:layout_alignParentTop="true"
                android:text="12-11-2012"
                android:textSize="12sp"
                 >
            </Button>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/focusskulistview_4C"
            android:layout_width="0dip"
            android:layout_height="55sp"
            android:layout_marginBottom="1sp"
            android:layout_weight=".13"
            android:background="#f1f1f1" >

            <Button
                android:id="@+id/dom2"
                android:layout_width="fill_parent"
                android:layout_height="55sp"
                android:text="12-11-2012"
                android:textSize="12sp" >
            </Button>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/focusskulistview_4C"
            android:layout_width="0dip"
            android:layout_height="55sp"
            android:layout_marginBottom="1sp"
            android:layout_weight=".13"
            android:background="#f1f1f1" >

            <Button
                android:id="@+id/dom3"
                android:layout_width="fill_parent"
                android:layout_height="55sp"
                android:layout_alignParentTop="true"
                android:text="12-11-2012"
                android:textSize="12sp" >
            </Button>
        </RelativeLayout>
    </LinearLayout>

    <Spinner
        android:id="@+id/skureasonspinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/focusskulistview_chartcontentarea"
        android:layout_below="@+id/focusskulistview_chartcontentarea" />

</RelativeLayout>